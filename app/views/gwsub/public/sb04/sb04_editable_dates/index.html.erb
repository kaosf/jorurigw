<%#*<h1>Gwsub::Public::Sb04::Sb04EditableDates#index</h1>%>
<%#*<p>Find me in app/views/gwsub/public/sb04/sb04_editable_dates/index.html.erb</p>%>
<%
%>

<div class="gwsub sb04">
<%= render :partial => 'gwsub/public/sb04/sb04_piece_header' %>
<div class="pieceBanner">
<%= render :partial => 'gwsub/public/sb04/sb04_subfunction_select_menu' %>
<%= render :partial => 'gwsub/public/sb04/sb04_level3menu_header' %>
</div>
<div class="pieceBody">
<%= render :partial => 'gwsub/public/sb04/sb04_editable_dates/display_name' %>
<%= render :partial => 'gwsub/public/sb04/sb04_editable_dates/search_with_fyear' %>
<%= div_notice %>
  <table class="index">
    <tr>
      <th></th>
      <th>年度</th>
      <th>編集開始日</th>
      <th>編集終了日</th>
      <th>見出し日付</th>
    </tr>
    <% @items.each do |item|  %>
    <tr <%= cycle '', 'class="cycle"' %>>
      <td><%= link_to '詳細' , "#{Site.current_node.public_uri}#{item.id}#{@param}" %></td>
      <td><%= item.fyear_markjp %></td>
      <td><%= Gw.datetime_str(item.start_at) %></td>
      <td><%= Gw.datetime_str(item.end_at) %></td>
      <td><%= Gw.date_str(item.headline_at) %></td>
    </tr>
    <% end %>
  </table>
<%= paginate @items %>
</div>
</div>
