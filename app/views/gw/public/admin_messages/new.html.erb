<%#*<h1>Gw::Public::AdminMessages#new</h1>%>
<%#*<p>Find me in app/views/gw/public/admin_messages/new.html.erb</p>%>

<div class="piece AdminMassages">
<%= render :partial => 'header_str' %>

  <div class="pieceBody">

<% form_for :item, :url => "#{Site.current_node.public_uri}", :html => {:method => :post} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>

<%= submit_for(f, {:caption => '保存'}) %>

<% end %>

</div><!--pieceBody-->
</div><!--piece admin_messages-->
