<%#
@cabinet_title は、Doclibrary::Public::CabinetsController で初期化しています。
%>
<div class="piece docLibrary">
<div class="pieceHeader">
<h2><%= @cabinet_title %></h2>
</div>
<div class="pieceBody">
<% form_for :item, :url => "#{Site.current_node.public_uri}create/", :html => {:multipart => true, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>

<%= submit_for_update(f, :js_submitter => 'my_submit()') %>

<% end %>
</div>
</div>
