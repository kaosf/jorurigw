<div class="bbsDocList circular setting">
<div class="pieceHeader">
<h2 class="pieceTitle"><%=@system_title%> 削除設定</h2>
</div><!--pieceHeader-->
<!--カラム用タグ-->
<div class="pieceBanner">
</div>
<div class="pieceBody">

  <div class="titleLine">
<span class="title"><%=@system_title%> 削除設定</span>
</div>

<% form_for :item, :url => @item.update_path ,:html => {:method=>:put , :multipart => true, :id => 'itemForm'} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>

<div class="submitters">
<%=f.submit("保存")%>
</div>

<% end %>

</div><!--pieceBody-->
<!--戻るボタン-->
<div class="btReturn">
  <%=link_to '管理者設定へ戻る', '/gw/admin_settings' %>
</div>
</div><!--piece gwbbs-->
