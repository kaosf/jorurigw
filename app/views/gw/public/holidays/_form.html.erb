<% # app/views/gw/public/holidays/_form.html.erb
style = 'width:30em;'
%>
<%= Gw.required_head %>
<table class="show">
  <tr>
    <th>年月日<%= required %></th>
    <td><%= date_picker7 f, 'st_at', :errors=>@item.errors, :mode=>:date, :value=>@item.st_at %></td>
  </tr>
  <tr>
    <th>休日名<%= required %></th>
    <td><%= f.text_field("title", :style => style) %></td>
  </tr>
</table>


