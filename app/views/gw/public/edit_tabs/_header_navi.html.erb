<%#*<p>Find me in app/views/gw/public/edit_tabs/_header_navi.html.erb</p>%>
<div class="navi">
<span style="margin-right:3em;" class="Menu">
  <%= link_to '全一覧' , "#{Site.current_node.public_uri}list?pid=0" %>
</span>
<% tree = @parent.parent_tree %>
<% tree.each_with_index do |c, k| %>
  <%=link_to c.name , "#{Gw.chop_with(Site.current_node.public_uri,'/')}?pid=#{c.id}" %>
  <% if tree.size != k + 1 %>&gt;<% end %>
<% end %>
</div>
