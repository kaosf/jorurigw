<div class="piece gwfaq">
<%= render :partial => 'header_str' %>
<div class="pieceBody">

<% form_for :item, :url => "#{Site.current_node.public_uri}create/?parent_id=#{params[:parent_id]}&title_id=#{params[:title_id]}", :html => {:method => :post} do |f| %>

<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>

<%= submit_for_create f %>

<% end %>

</div><!--pieceBody-->
</div><!--piece gwfaq-->
