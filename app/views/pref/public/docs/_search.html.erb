<% form_tag '', :method => :get, :class => 'search' do %>

<table>
  
  <tr>
    <th>キーワード</th>
    <td><%= text_field_tag('s_keyword', params[:s_keyword]) %></td>
    <td class="submitters">
      <%= submit_tag '検索',     :name => :search %>
      <%= submit_tag 'リセット', :name => :reset %>
    </td>
  </tr>
  
</table>

