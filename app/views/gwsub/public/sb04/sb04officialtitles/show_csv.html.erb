     <tr>
       <th>備考</th><td><%=h @item.remarks -%></td>
     </tr>
    </tbody>
  </table>
  <div class="btReturn">
  <%= link_to '一覧に戻る',"#{Gw.chop_with(Site.current_node.public_uri,'/')}/index_csv#{@param}" %>
