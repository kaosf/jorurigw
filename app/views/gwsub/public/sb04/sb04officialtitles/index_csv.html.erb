<%# app/views/gwsub/public/sb04/sb04_check_officialtitles/index %>
<!-- コード管理 -->
<div class="gwsub sb04">
<%= render :partial => 'gwsub/public/sb04/sb04_piece_header' %>
<div class="pieceBanner">
<%= render :partial => 'gwsub/public/sb04/sb04_subfunction_select_menu' %>
<%= render :partial => 'gwsub/public/sb04/sb04_level3menu_header' %>
</div>
<div class="pieceBody">
<%= render :partial => 'gwsub/public/sb04/sb04officialtitles/display_name' %>
<%= render :partial => 'gwsub/public/sb04/search_check_view' %>
<%= div_notice %>
<%#= render :partial => 'gwsub/public/sb04/sb04_common_index_view' , :locals => {:link_to_action => nil ,:link_to_show => 1} %>
<% if @items.length == 0 %>
  <%= Gw.div_notice("表示する項目はありません。") -%>
<% else %>
  <table class="index">
    <tr>
      <th></th>
      <th>年度</th>
      <th>職名コード</th>
      <th>職名</th>
      <th>備考</th>
    </tr>
    <% @items.each do |item|  %>
    <tr <%= cycle '', 'class="cycle"' %>>
      <td><%= link_to '詳細' , "#{Site.current_node.public_uri}#{item.id}/show_csv#{@param}" %></td>
      <td><%= item.fyear_markjp %></td>
      <td><%= item.code %></td>
      <td><%= item.name %></td>
      <td><%= item.remarks %></td>
    </tr>
    <% end %>
  </table>
<% end %>
<%= paginate @items %>
</div>
</div>
