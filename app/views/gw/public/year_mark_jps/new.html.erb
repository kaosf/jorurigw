<%
@create_path = Gw.chop_with("#{Site.current_node.public_uri}","/")
%>
<div class="piece YearMarkJp">
<%= render :partial => 'gw/public/year_mark_jps/header' %>
<div class="pieceBody">
<% form_for :item, :url => @create_path, :html => {:method => :post, :multipart => true} do |f| %>
<%= render :partial => 'form', :locals => {:f => f} %>
<%= submit_for(f, {:caption => '保存'}) %>
<% end %>
</div>
</div