<%

link_s = %Q(<span class="current_menu">)
link_s_end = %Q(</span>)

lnks1 = (@hedder3lnk == 1 ? "#{link_s}新規作成#{link_s_end}" : "<span>新規作成</span>")
lnks2 = (@hedder3lnk == 2 ? "#{link_s}一覧#{link_s_end}" : "<span>一覧</span>")

concat <<-EOL
<div class="menu">
#{!@is_admin ? '' : link_to(lnks1, "#{@uri_base}/new?cls=#{@cls}")}
#{link_to lnks2, "#{@uri_base}?cls=#{@cls}"}
</div>
EOL

%>
