<%= render :partial => 'folder_name' %>

<% form_for :item, :url => @item.update_path, :html => {:multipart => true, :id => 'itemForm'} do |f| %>

  <%= f.error_messages %>

  <%= render :partial => 'form', :locals => {:f => f} %>

  <%= submit_for_update(f) unless @parent_config.length == 0 %>
  <%= submit_for_update(f, :js_submitter => 'my_submit()') if @parent_config.length == 0 %>

<% end %>
