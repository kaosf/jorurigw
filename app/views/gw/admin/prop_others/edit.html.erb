<%
@prop_master = @genre
%>
<div class="piece prop form">
  <div class="pieceHeader">
    <%= render :partial => '/gw/admin/prop_others/piece_header' -%>
  </div>
  <div class="pieceBody">
    <div class="pieceBodyHeader">
      <%= render(:partial => '/gw/admin/schedules/schedule_bt_box', :locals=>{:d=>Date.today, :mode=>'month', :genre=>@genre}) -%>
    </div>
    <div class="pieceBodyBody">
      <%= div_notice -%>
      <%= form_for :item, :url => "#{@uri_base}/#{@item.id}?cls=#{@cls}", :html => {:method => :put, :multipart => true, :id => 'itemForm'} do |f| -%>
        <%= f.error_messages -%>
        <%= render :partial => '/gw/admin/prop_others/form', :locals => {:f => f} -%>
        <%= raw(submit_for_update f, :js_submitter => 'my_submit()') -%>
      <% end -%>
    </div>
  </div>
</div>
