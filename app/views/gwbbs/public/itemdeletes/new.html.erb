<div class="piece bbs">
<div class="pieceHeader">
<h2>掲示板記事削除</h2>
</div>
<div class="pieceBody">
<div class="editingBox">
<span class="docLink"><%=link_to('管理者設定 &gt;&gt;', '/gw/admin_settings') %></span>
</div>

<% form_for :item, :url => "#{admin_item_deletes_path}/create" , :html => {:method => :post} do |f| %>
<%= f.error_messages %>
<%= render :partial => 'form', :locals => {:f => f} %>

<div class="submitters">
<%=f.submit("保存")%>
</div>

<% end %>
</div>


<div class="btReturn">
  <%=link_to('処理選択へ戻る', admin_item_deletes_path) %>
</div>
</div><!--piece gwbbs-->